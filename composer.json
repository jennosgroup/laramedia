{
    "name": "jennosgroup/laramedia",
    "type": "package",
    "description": "A media package for Laravel.",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Jenry Ollivierre",
            "email": "jenry@jenryollivierre.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ramsey/uuid": "^4.7",
        "intervention/image": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "JennosGroup\\Laramedia\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JennosGroup\\Laramedia\\Providers\\AppServiceProvider"
            ],
            "aliases": {
                "LaramediaConfig": "JennosGroup\\Laramedia\\Facades\\Config"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.45"
    }
}
